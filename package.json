{
  "name": "@antetown/games",
  "version": "1.0.0",
  "private": true,
  "description": "AnteTown Games Monorepo - All casino games for the AnteTown platform",
  "workspaces": [
    "packages/*",
    "games/*",
    "games/*/backend",
    "games/*/frontend"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "build:games": "npm run build --workspace games/*",
    "build:packages": "npm run build --workspace packages/*",
    "clean": "npm run clean --workspaces --if-present",
    "test": "npm run test --workspaces --if-present"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nhillen/AnteTown-Games.git"
  },
  "author": "AnteTown",
  "license": "MIT"
}
